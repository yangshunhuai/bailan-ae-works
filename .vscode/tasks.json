{
    "tasks": [
      {
        "type": "cbuild",
        "label": "C/C++: gcc.exe build active file",
        "command": "gcc.exe",
        "args": ["-g", "${file}", "-o", "${fileDirname}\\${fileBasenameNoExtension}.exe"],
        "options": {
          "cwd": "${fileDirname}"
        },
        "problemMatcher": ["$gcc"],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "detail": "compiler: gcc.exe"
      }
    ],
    "version": "2.0.0"
  }